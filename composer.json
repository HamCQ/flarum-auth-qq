  "keywords": [
    "flarum",
    "authentication",
    "QQ"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "*",
    "fof/components": "^1.0.0",
    "league/oauth2-client": "^2.2",
    "laravel/helpers": "^v1.4.1",
    "ext-json": "*"
  },
  "authors": [
    {
      "name": "Emin.lin",
      "email": "emin.lin@live.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "HamCQ\\QQAuth\\": "src/"
    }
  },
  "extra": {
    "flarum-extension": {
      "title": "QQ Login",
      "icon": {
        "name": "fab fa-qq",
        "backgroundColor": "#14baf8",
        "color": "#fff"
